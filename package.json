{
  "name": "testingcourse",
  "version": "1.0.0",
  "description": "testing course project",
  "main": "index.js",
  "type": "module",
  "bin": {
    "coveralls": "./bin/coveralls.js"
  },
  "scripts": {
    "test": "mocha --exit",
    "coverage": "nyc npm run test && shx cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true,
    "include": [
      "codes"
    ],
    "exclude": [
      "codes\\.internal"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "mocha": "^8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "mochawesome": "^6.2.1",
    "nyc": "^15.1.0",
    "shx": "^0.3.3"
  },
  "dependencies": {
    "lcov-parse": "^1.0.0"
  }
}
